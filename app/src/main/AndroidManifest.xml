<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demoapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".common.DemoApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".history_screen.HistoryScreen"
           />
        <activity android:name=".profile_screen.ProfileScreen" />
        <activity android:name=".user_screen.UserScreen" />
        <activity
            android:name=".sign_up_screen.SignUpScreen"
            android:parentActivityName=".home_screen.MainActivity" />
        <activity android:name=".admin_screen.AdminScreen" />
        <activity android:name=".home_screen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>